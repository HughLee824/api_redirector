# Google Maps API Configuration
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Authentication (format: key:name:permissions separated by semicolon)
# Example: API_KEYS=abc123:user1:maps,geocode;def456:user2:maps
API_KEYS=your_api_key_1:user1:maps,geocode;your_api_key_2:user2:maps

# Rate Limiting
DEFAULT_RATE_LIMIT=100
DEFAULT_RATE_WINDOW=3600

# Logging
LOG_LEVEL=info
ENABLE_REQUEST_LOGGING=true
